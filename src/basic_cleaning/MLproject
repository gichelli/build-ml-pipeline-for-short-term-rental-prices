        type: string

      output_type:
        description: This is output type ## ADD DESCRIPTION
        type: string

      output_description:
        description: Description about the artifact ## ADD DESCRIPTION
        type: string

      min_price:
        description: This is min price ## ADD DESCRIPTION
        type: float

      max_price:
        description: This is max price ## ADD DESCRIPTION
        type: float


